                  <Button fx:id="stergeSimptomButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onPressStergeSimptom" prefHeight="30.0" prefWidth="80.0" text="Sterge" />
