<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="medproject.medclient.graphicalInterface.loginWindow.loginScene.LoginController">
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" maxWidth="95.0" minWidth="10.0" prefWidth="92.0" />
      <ColumnConstraints hgrow="NEVER" maxWidth="118.0" minWidth="10.0" prefWidth="108.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints maxHeight="212.0" minHeight="10.0" prefHeight="55.0" vgrow="NEVER" />
    <RowConstraints maxHeight="214.0" minHeight="10.0" prefHeight="54.0" vgrow="NEVER" />
      <RowConstraints maxHeight="508.0" minHeight="10.0" prefHeight="72.0" vgrow="NEVER" />
      <RowConstraints maxHeight="508.0" minHeight="10.0" prefHeight="65.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <Label text="Username:" GridPane.halignment="CENTER" />
      <Label text="Password:" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <TextField fx:id="username_field" promptText="username" GridPane.columnIndex="1" GridPane.halignment="CENTER">
         <GridPane.margin>
            <Insets right="5.0" />
         </GridPane.margin>
      </TextField>
      <TextField fx:id="password_field" promptText="password" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets right="5.0" />
         </GridPane.margin>
      </TextField>
      <Button fx:id="login_button" disable="true" mnemonicParsing="false" onAction="#onLoginPress" prefHeight="25.0" prefWidth="121.0" text="Log in" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#onRegisterPress" text="Register" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
